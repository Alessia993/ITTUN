<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">
	<application>
		<locale-config>
			<default-locale>it</default-locale>
			<supported-locale>en</supported-locale>
			<supported-locale>it</supported-locale>
			<supported-locale>fr</supported-locale>
		</locale-config>
		<message-bundle>Resources</message-bundle>
		<resource-bundle>
			<base-name>Resources</base-name>
			<var>msg</var>
		</resource-bundle>
		<view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
		<view-handler>com.infostroy.paamns.web.handlers.CustomViewHandler</view-handler>
	</application>
	<lifecycle>
		<phase-listener id="nocache">com.infostroy.paamns.web.listeners.CachePhaseListener</phase-listener>
	</lifecycle>
</faces-config>
